@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        @{
            var grid = new WebGrid(source: ViewBag.movies);
        }
        @grid.GetHtml(
            columns: grid.Columns(
            grid.Column("Title"),
            grid.Column("Genre"),
            grid.Column("Year")
            )
        )
    </div>
</body>
</html>
